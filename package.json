{
  "name": "linkedin-roast",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:frontend\" \"bun run dev:server\"",
    "dev:frontend": "vite",
    "dev:server": "NODE_ENV=development PORT=8787 bun run --watch server/index.ts",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "start": "bun run server/index.ts"
  },
  "dependencies": {
    "pdf-parse": "1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  }
}
